#!/usr/bin/env bash

iptables -F
iptables -t nat -F
iptables -t nat -L
iptables -t nat -A PREROUTING  -m addrtype  --dst-type LOCAL  --jump STARPHLEET
iptables -t nat -A STARPHLEET -p tcp --dport 80 ! --in-interface lxcbr0 --jump DNAT --to-destination 10.0.3.56:80
iptables -t nat -A OUTPUT -m addrtype --dst-type LOCAL -j STARPHLEET
#iptables -t nat -A STARPHLEET -p tcp --dport 80 --jump DNAT --to-destination 10.0.3.56:80
#iptables -t nat -A OUTPUT -m addrtype --dst-type LOCAL ! --dst 127.0.0.0/8 --jump STARPHLEET
#iptables -t nat -A POSTROUTING -s 10.0.3.56 ! -d 10.0.3.56 -j MASQUERADE
#iptables -t nat -A POSTROUTING --out-interface eth0 -j MASQUERADE
#iptables -t nat -A POSTROUTING --out-interface eth1 -j MASQUERADE

iptables -t nat -A POSTROUTING -s 10.0.3.0/24 -j MASQUERADE
